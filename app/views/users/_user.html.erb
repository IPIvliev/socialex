        <div class='users'>

          <div class='profile_image'>
            <%= link_to (image_tag 'https://graph.facebook.com/'+user.uid+'/picture?type=normal&width=86&height=86'), user, :class => "img-thumbnail" %>
          </div>
          <div class='profile_name'>
            <div class="price">
              <span class="amount"><%= Stock.where("host_id = ?", user.id).sum(:amount) %> осталось </span><br>
              

              <%= Stock.stockprice(user) %> руб. (<span class="green">+</span>)
            </div>
            <%= user.name %> 
          </div>
        </div>